#include <stdio.h>
#include <stdlib.h>
typedef struct Node
{
	int val;
	struct Node *next;
}Node;

Node *list(void)
{
	Node *p, *q, *head;
	int i, n, a;
	scanf("%d", &n);
	head = NULL;
	for(i = 1; i<=n; i++){
		scanf("%d", &a);
		p = (Node*)malloc(sizeof(Node));
		p->val = a;
		p->next = NULL;
		if(head==NULL)
			head = p;
		else
			q->next = p;
		q = p;
	}
	return head;
}


 int main()
 {
	Node *list1, *list2;
	list1 = Node *list(void);
	list2 = Node *list(void);
	
	
  return 0;
}
  
  
  

